<?xml version="1.0" encoding="utf-8"?>

<!-- https://unfccc.int/process/transparency-and-reporting/greenhouse-gas-data/greenhouse-gas-data-unfccc/global-warming-potentials -->
<!-- GWP for 100-year time horizon, ipcc95(SAR), ipcc07(AR4) -->
<gwp>
  <ghg-class name="Carbon dioxide">
    <ghg name="CO2" gwp20="1" gwp100="1"/>
  </ghg-class>
  <ghg-class name="Methane">
    <ghg name="CH4" gwp20="56" gwp100="21"/>
  </ghg-class>
  <ghg-class name="Nitrous oxide">
    <ghg name="N2O" gwp20="280" gwp100="310"/>
  </ghg-class>
  <ghg-class name="HFCs">
    <ghg name="HFC23" gwp20="9100" gwp100="11700"/>
    <ghg name="HFC32" gwp20="2100" gwp100="650"/>
    <ghg name="HFC134a" gwp20="3400" gwp100="1300"/>
  </ghg-class>
  <ghg-class name="Sulphur hexafluoride">
    <ghg name="SF6" gwp20="16300" gwp100="23900"/>
  </ghg-class>
  <ghg-class name="PFCs">
    <ghg name="C2F6" gwp20="6200" gwp100="9200"/>
    <ghg name="CF4" gwp20="4400" gwp100="6500"/>
  </ghg-class>
</gwp>